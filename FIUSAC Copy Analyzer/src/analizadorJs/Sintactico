terminal String IDENTIFICADOR, BPCADENA, DD, BPFALSE, BPTRUE, BPAUMENTO, BPDISMINUIR;
terminal String LINECOMMENT, MULTILINECOMMENT, BPRUTA;


non terminal ini, procesos, parametros, variable, impresion, opmate, concatenar, condicion, forvariable, condicionfor, foraumento, casos, signos, comentario, siguiente;

