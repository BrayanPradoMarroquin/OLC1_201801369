casos::=casos BPCASE DD BPDOSPUNTOS procesos BPBREAK BPPTCOMA {::}             
       |casos BPDEFAULT BPDOSPUNTOS procesos BPBREAK BPPTCOMA {::}
       |casos BPDEFAULT BPDOSPUNTOS procesos {::}
       |casos BPCASE BPCADENA BPDOSPUNTOS procesos BPBREAK BPPTCOMA {::}
       |BPCASE DD BPDOSPUNTOS procesos BPBREAK BPPTCOMA {::}
       |BPDEFAULT BPDOSPUNTOS procesos BPBREAK BPPTCOMA {::}
       |BPDEFAULT BPDOSPUNTOS procesos {::}
       |BPCASE BPCADENA BPDOSPUNTOS procesos BPBREAK BPPTCOMA {::}
;

impresion::=impresion BPMAS DD {::}
           |impresion BPMAS BPCADENA {::}
           |impresion BPMAS BPNOTEQUALS IDENTIFICADOR {::}
           |impresion BPMAS IDENTIFICADOR {::}
           |impresion BPMAS BPPARIZQ opmate BPPARDER
           |BPCADENA {::}
           |IDENTIFICADOR {::}
           |DD {::}
           |BPGUION DD {::}
           |BPGUION BPPARIZQ opmate BPPARDER {::}
           |BPGUION BPPARIZQ DD BPPARDER {::}
           |BPGUION BPPARIZQ IDENTIFICADOR BPPARDER {::}
           |BPPARIZQ opmate BPPARDER {::}
           |IDENTIFICADOR BPAUMENTO {::}
           |IDENTIFICADOR BPDISMINUIR {::}
           |BPNOTEQUALS IDENTIFICADOR {::}
;

opmate::=opmate BPPARIZQ opmate BPPARDER {::}
        |opmate DD BPPON DD {::}
        |opmate DD BPDIVIDIR DD {::}
        |opmate DD BPMOD DD {::}
        |opmate DD BPPOR DD {::}
        |opmate DD BPMAS DD {::}
        |opmate DD BPGUION DD {::}
        |opmate DD BPPON IDENTIFICADOR {::}
        |opmate DD BPDIVIDIR IDENTIFICADOR {::}
        |opmate DD BPMOD IDENTIFICADOR {::}
        |opmate DD BPPOR IDENTIFICADOR {::}
        |opmate DD BPMAS IDENTIFICADOR {::}
        |opmate DD BPGUION IDENTIFICADOR {::}

        |BPPARIZQ opmate BPPARDER {::}
        |DD BPPON DD {::}
        |DD BPDIVIDIR DD {::}
        |DD BPMOD DD {::}
        |DD BPPOR DD {::}
        |DD BPMAS DD {::}
        |DD BPGUION DD {::}
        |DD BPPON IDENTIFICADOR {::}
        |DD BPDIVIDIR IDENTIFICADOR {::}
        |DD BPMOD IDENTIFICADOR {::}
        |DD BPPOR IDENTIFICADOR {::}
        |DD BPMAS IDENTIFICADOR {::}
        |DD BPGUION IDENTIFICADOR {::}
;            