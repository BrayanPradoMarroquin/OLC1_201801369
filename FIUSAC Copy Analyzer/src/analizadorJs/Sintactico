concatenar::=concatenar BPMAS IDENTIFICADOR {::}
          |concatenar BPMAS DD {::}
          |concatenar BPMAS BPCADENA {::}
          |concatenar BPMAS BPRUTA {::}
          |IDENTIFICADOR BPMAS DD {::}
          |IDENTIFICADOR BPMAS BPCADENA {::}
          |IDENTIFICADOR BPMAS BPRUTA {::}
          |DD BPMAS IDENTIFICADOR {::}
          |BPCADENA BPMAS IDENTIFICADOR {::}
          |BPRUTA BPMAS IDENTIFICADOR {::}
          |IDENTIFICADOR BPMAS IDENTIFICADOR {::}
;

forvariable::=BPVAR IDENTIFICADOR BPEQUAL variable {::}
             |BPLET IDENTIFICADOR BPEQUAL variable {::}
             |IDENTIFICADOR BPEQUAL IDENTIFICADOR BPPTCOMA {::}
             |IDENTIFICADOR opmate BPPTCOMA {::}
             |IDENTIFICADOR BPPTCOMA {::}
             |IDENTIFICADOR BPEQUAL variable {::}
;

condicionfor::=IDENTIFICADOR BPMAYOR IDENTIFICADOR BPPTCOMA {::}
             |IDENTIFICADOR BPMENOR IDENTIFICADOR BPPTCOMA {::}
             |IDENTIFICADOR BPMAYOREQUALS IDENTIFICADOR BPPTCOMA {::}
             |IDENTIFICADOR BPMENOREQUALS IDENTIFICADOR BPPTCOMA {::}
             |IDENTIFICADOR BPMAYOR DD BPPTCOMA {::}
             |IDENTIFICADOR BPMENOR DD BPPTCOMA {::}
             |IDENTIFICADOR BPMAYOREQUALS DD BPPTCOMA {::}
             |IDENTIFICADOR BPMENOREQUALS DD BPPTCOMA {::}
             |IDENTIFICADOR BPMAYOR opmate BPPTCOMA {::}
             |IDENTIFICADOR BPMENOR opmate BPPTCOMA {::}
             |IDENTIFICADOR BPMAYOREQUALS opmate BPPTCOMA {::}
             |IDENTIFICADOR BPMENOREQUALS opmate BPPTCOMA {::}
;

foraumento::=IDENTIFICADOR BPAUMENTO {::}
         |IDENTIFICADOR BPDISMINUIR {::}
         |IDENTIFICADOR BPMAS BPEQUAL DD {::}
//         |IDENTIFICADOR BPMENOR BPEQUAL DD {::}
;

condicion::=condicion BPCADENA BPEQUALCONDITION BPCADENA {::}
          |condicion DD BPEQUALCONDITION DD {::}
          |condicion DD BPMAYOR DD {::}
          |condicion DD BPMENOR DD {::}
          |condicion DD BPMAYOREQUALS DD {::}
          |condicion DD BPMENOREQUALS DD {::}
          
          |condicion opmate BPEQUALCONDITION DD {::}
          |condicion opmate BPMAYOR DD {::}
          |condicion opmate BPMENOR DD {::}
          |condicion opmate BPMAYOREQUALS DD {::}
          |condicion opmate BPMENOREQUALS DD {::}
          |condicion opmate BPEQUALCONDITION IDENTIFICADOR {::}
          |condicion opmate BPMAYOR IDENTIFICADOR {::}
          |condicion opmate BPMENOR IDENTIFICADOR {::}
          |condicion opmate BPMAYOREQUALS IDENTIFICADOR {::}
