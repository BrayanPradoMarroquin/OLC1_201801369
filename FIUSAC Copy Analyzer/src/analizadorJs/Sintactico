casos::=casos BPCASE:var1 DD BPDOSPUNTOS procesos BPBREAK BPPTCOMA {:Informacion.add(new NodoinformacionJS("Sentencia", var1));:}             
       |casos BPDEFAULT:var1 BPDOSPUNTOS procesos BPBREAK BPPTCOMA {:Informacion.add(new NodoinformacionJS("Sentencia", var1));:}
       |casos BPDEFAULT:var1 BPDOSPUNTOS procesos {:Informacion.add(new NodoinformacionJS("Sentencia", var1));:}
       |casos BPCASE:var1 BPCADENA BPDOSPUNTOS procesos BPBREAK BPPTCOMA {:Informacion.add(new NodoinformacionJS("Sentencia", var1));:}
       |BPCASE:var1 DD BPDOSPUNTOS procesos BPBREAK BPPTCOMA {:Informacion.add(new NodoinformacionJS("Sentencia", var1));:}
       |BPDEFAULT:var1 BPDOSPUNTOS procesos BPBREAK BPPTCOMA {:Informacion.add(new NodoinformacionJS("Sentencia", var1));:}
       |BPDEFAULT:var1 BPDOSPUNTOS procesos {:Informacion.add(new NodoinformacionJS("Sentencia", var1));:}
       |BPCASE:var1 BPCADENA BPDOSPUNTOS procesos BPBREAK BPPTCOMA {:Informacion.add(new NodoinformacionJS("Sentencia", var1));:}
;

impresion::=impresion BPMAS DD:var1 {:RESULT=var1;:}
           |impresion BPMAS BPCADENA:var1 {:RESULT=var1;:}
           |impresion BPMAS BPNOTEQUALS IDENTIFICADOR:var1 {:RESULT=var1;:}
           |impresion BPMAS IDENTIFICADOR:var1 {:RESULT=var1;:}
           |impresion BPMAS BPPARIZQ opmate BPPARDER
           |BPCADENA:var1 {:RESULT=var1;:}
           |IDENTIFICADOR:var1 {:RESULT=var1;:}
           |DD {::}
           |BPGUION DD {::}
           |BPGUION BPPARIZQ opmate BPPARDER {::}
           |BPGUION BPPARIZQ DD BPPARDER {::}
           |BPGUION BPPARIZQ IDENTIFICADOR BPPARDER {::}
           |BPPARIZQ opmate BPPARDER {::}
           |IDENTIFICADOR BPAUMENTO {::}
           |IDENTIFICADOR BPDISMINUIR {::}
           |BPNOTEQUALS IDENTIFICADOR {::}
;

